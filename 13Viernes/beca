#include <stdio.h>
#include <stdlib.h>


int main()
{
    int A ;
    int B ;
    printf ("Ingrese su promedio \n");
     scanf("%d",&A);
      printf ("\ Ingrese la cantidad de materias reprobadas \n");
     scanf("%d",&B);
    if (A>=85&&B==0) {
        printf ("Obtiene la beca");
        }
        else{
         printf ("No obtiene la beca");
        }
    return 0;


}
